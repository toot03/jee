<?xml version="1.0" encoding="UTF-8"?>
<web-app
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">
  <display-name>LabSERVLET_04-Session</display-name>
  <description>LabSERVLET_04-Session</description>
  <welcome-file-list>
    <welcome-file>Lancement.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>Servlet1</servlet-name>
    <servlet-class>pack1.HttpSessionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlet1</servlet-name>
    <url-pattern>/HttpSessionServlet</url-pattern>
  </servlet-mapping>
  
</web-app>